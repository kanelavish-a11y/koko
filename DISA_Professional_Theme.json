{
    "name": "DISA Professional Theme",
    "dataColors": [
        "#17A2B8",
        "#1E4D78",
        "#2D9CDB",
        "#6FCF97",
        "#9B51E0",
        "#F2994A",
        "#56CCF2",
        "#27AE60",
        "#EB5757",
        "#BB6BD9"
    ],
    "good": "#27AE60",
    "neutral": "#F2C94C",
    "bad": "#EB5757",
    "maximum": "#17A2B8",
    "center": "#F2C94C",
    "minimum": "#E8F4F8",
    "null": "#BDBDBD",
    "background": "#FFFFFF",
    "foreground": "#333333",
    "tableAccent": "#17A2B8",
    "firstLevelElements": "#333333",
    "secondLevelElements": "#828282",
    "thirdLevelElements": "#F2F2F2",
    "fourthLevelElements": "#BDBDBD",
    "secondaryBackground": "#F9F9F9",
    "textClasses": {
        "callout": {
            "fontSize": 32,
            "fontFace": "Segoe UI",
            "color": "#333333"
        },
        "title": {
            "fontSize": 12,
            "fontFace": "Segoe UI Semibold",
            "color": "#333333"
        },
        "header": {
            "fontSize": 12,
            "fontFace": "Segoe UI Semibold",
            "color": "#333333"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Segoe UI",
            "color": "#4F4F4F"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "background": [{
                    "show": true,
                    "color": {"solid": {"color": "#FFFFFF"}},
                    "transparency": 0
                }],
                "border": [{
                    "show": false
                }],
                "visualHeader": [{
                    "show": true,
                    "background": {"solid": {"color": "#FFFFFF"}},
                    "transparency": 100
                }],
                "title": [{
                    "show": true,
                    "fontColor": {"solid": {"color": "#333333"}},
                    "fontSize": 14,
                    "fontFamily": "Segoe UI Semibold"
                }],
                "outspacePane": [{
                    "backgroundColor": {"solid": {"color": "#FFFFFF"}},
                    "transparency": 0
                }]
            }
        },
        "card": {
            "*": {
                "labels": [{
                    "color": {"solid": {"color": "#333333"}},
                    "fontSize": 28,
                    "fontFamily": "Segoe UI Light"
                }],
                "categoryLabels": [{
                    "show": true,
                    "color": {"solid": {"color": "#828282"}},
                    "fontSize": 10
                }],
                "background": [{
                    "show": true,
                    "color": {"solid": {"color": "#FFFFFF"}},
                    "transparency": 0
                }]
            }
        },
        "tableEx": {
            "*": {
                "grid": [{
                    "gridVertical": true,
                    "gridVerticalColor": {"solid": {"color": "#F2F2F2"}},
                    "gridHorizontal": true,
                    "gridHorizontalColor": {"solid": {"color": "#F2F2F2"}},
                    "rowPadding": 6
                }],
                "columnHeaders": [{
                    "fontColor": {"solid": {"color": "#333333"}},
                    "backColor": {"solid": {"color": "#F9F9F9"}},
                    "fontSize": 10,
                    "fontFamily": "Segoe UI Semibold"
                }],
                "values": [{
                    "fontColorPrimary": {"solid": {"color": "#333333"}},
                    "backColorPrimary": {"solid": {"color": "#FFFFFF"}},
                    "fontSize": 10
                }]
            }
        },
        "page": {
            "*": {
                "background": [{
                    "color": {"solid": {"color": "#F5F5F5"}},
                    "transparency": 0
                }],
                "outspace": [{
                    "color": {"solid": {"color": "#F5F5F5"}}
                }]
            }
        }
    }
}
